#!/usr/bin/gnuplot

reset
set terminal fig color
set output "table11.fig"
set xlabel "Percentage of Data Used for Training"
set ylabel "RMSE"
set size 0.7,0.5
set key right outside
set logscale y
set style data linespoints

plot "table11.dat" using 1:2 title "LI", \
"" using 1:3 title "TR-MF", \
"" using 1:4 title "AKE", \
"" using 1:5 title "DESM", \
"" using 1:6 title "STI", \
"" using 1:7 title "MI"
#